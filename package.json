{
  "name": "spyfall",
  "version": "2.0.0",
  "description": "spyfall",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/adrianocola/spyfall"
  },
  "keywords": [
    "spyfall"
  ],
  "engines": {
    "npm": ">=5",
    "node": ">=8"
  },
  "author": "Adriano Cola",
  "license": "ISC",
  "homepage": "http://adrianocola.com/spyfall",
  "scripts": {
    "postinstall": "patch-package",
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=development BUILD=true webpack --config config/webpack.dev.babel.js --color --progress --hide-modules --display-optimization-bailout",
    "build-prod": "cross-env NODE_ENV=production BUILD=true webpack --config config/webpack.prod.babel.js --color --progress --hide-modules --optimize-minimize --optimize-dedupe --display-optimization-bailout",
    "build:clean": "rimraf ./build",
    "firebase-deploy": "firebase deploy --only hosting -P default",
    "firebase-deploy-prod": "rm ./build/*.js.map ; firebase deploy --only hosting -P prod",
    "release": "npm run build && npm run deploy-functions && npm run deploy-translations",
    "release-prod": "npm run build-prod && npm run deploy-functions-prod && npm run deploy-translations-prod",
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --config config/webpack.dev.babel.js --port 4000",
    "clean": "npm run test:clean && npm run build:clean",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore",
    "lint:staged": "lint-staged",
    "eslint:fix": "eslint -- . --ignore-path .gitignore --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "deploy": "cp app/translations.js functions/lib/translations.js && firebase deploy --project default --only",
    "deploy-prod": "cp app/translations.js functions/lib/translations.js && firebase deploy --project prod --only",
    "deploy-functions": "cp app/translations.js functions/lib/translations.js && firebase deploy --project default --only functions",
    "deploy-functions-prod": "cp app/translations.js functions/lib/translations.js && firebase deploy --project prod --only functions",
    "copy-translations": "rimraf ./build/i18n && ncp ./public/i18n ./build/i18n",
    "deploy-translations": "npm run copy-translations && npm run firebase-deploy",
    "deploy-translations-prod": "npm run copy-translations && npm run firebase-deploy-prod"
  },
  "lint-staged": {
    "*.js": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts"
  },
  "pre-commit": "lint:staged",
  "dependencies": {
    "@hot-loader/react-dom": "^16.13.0",
    "animate.css": "^3.7.2",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.5.0",
    "classnames": "^2.2.6",
    "cross-env": "^5.2.1",
    "emotion": "^10.0.27",
    "firebase": "^7.16.1",
    "fontfaceobserver": "^2.1.0",
    "history": "^4.10.1",
    "i18next": "^17.3.1",
    "i18next-browser-languagedetector": "^3.1.1",
    "i18next-xhr-backend": "^3.2.2",
    "lodash": "^4.17.19",
    "react": "^16.13.1",
    "react-animated-css": "^1.2.1",
    "react-dropzone": "^10.2.2",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.12.21",
    "react-html-parser": "^2.0.2",
    "react-i18next": "^10.13.2",
    "react-icons": "^3.10.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-spinners": "^0.5.12",
    "react-toastify": "^5.5.0",
    "reactstrap": "^8.4.1",
    "redux": "^4.0.5",
    "redux-persist": "^5.10.0",
    "reselect": "^4.0.0",
    "sanitize.css": "^10.0.0",
    "shortid": "^2.2.15"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-decorators": "^7.10.1",
    "@babel/plugin-proposal-optional-chaining": "^7.10.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.10.1",
    "@babel/plugin-transform-react-constant-elements": "^7.10.1",
    "@babel/plugin-transform-react-inline-elements": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/register": "^7.10.1",
    "@ensemblebr/eslint-config": "^1.3.0",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-emotion": "^10.0.33",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-runtime": "^6.26.0",
    "circular-dependency-plugin": "^5.2.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^2.1.1",
    "dynamic-cdn-webpack-plugin": "^5.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-emotion": "^10.0.27",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eventsource-polyfill": "^0.9.6",
    "exports-loader": "^0.7.0",
    "file-loader": "^4.3.0",
    "firebase-tools": "^8.6.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^5.1.0",
    "imports-loader": "^0.8.0",
    "lint-staged": "^8.2.1",
    "mobx-react-devtools": "^6.1.1",
    "ncp": "^2.0.0",
    "node-plop": "^0.18.0",
    "node-sass": "^4.14.1",
    "null-loader": "^3.0.0",
    "patch-package": "^6.2.1",
    "plop": "^2.7.1",
    "pre-commit": "^1.2.2",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^2.7.1",
    "sass-loader": "^7.3.1",
    "shelljs": "^0.8.4",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^2.3.7",
